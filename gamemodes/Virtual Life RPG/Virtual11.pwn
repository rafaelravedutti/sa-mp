stock QtLadroesPL(){
    if(!PLIniciou) return 0;
	new result;
	for(new i = 0; i < MAX_SLOTS; i++){
	    if(Equipe[i] == 1 && MiniGame[i] == 1) result++;
	}
	return result;
}
stock QtPoliciaisPL(){
    if(!PLIniciou) return 0;
