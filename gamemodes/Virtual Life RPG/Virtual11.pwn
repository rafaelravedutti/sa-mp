	ResetWeapons(playerid);
	GameTextForPlayer(playerid,"~r~fuja dos ~n~policiais",3000,1);
	GangZoneShowForPlayer(playerid, PoliciaLadraoArea, COLOR_COP_SWAT);
}
public SetPolicialPL(playerid){
