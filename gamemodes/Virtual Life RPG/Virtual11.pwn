	    if(Equipe[i] == 2 && MiniGame[i] == 1) result++;
	}
	return result;
}
stock QtCapturadosPL(){
    if(!PLIniciou) return 0;
	new result;
	for(new i = 0; i < MAX_SLOTS; i++){
	    if(Capturado[i] == 1 && Equipe[i] == 1 && MiniGame[i] == 1) result++;
	}
	return result;
}
