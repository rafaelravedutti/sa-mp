	    	CreateExplosion(X - 1.0, Y, Z, 6, 50.0);
	    	CreateExplosion(X, Y + 1.0, Z, 6, 50.0);
	    	CreateExplosion(X, Y - 1.0, Z, 6, 50.0);
		}
	}
    else
	{
	    for(new i = 0; i < MAX_SLOTS; i++)
