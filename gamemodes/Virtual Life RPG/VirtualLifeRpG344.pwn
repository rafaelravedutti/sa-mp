stock TrancarVeiculosConfiscados(playerid){
	new str[128];
	for(new v = 0; v < AmountServerVehicles + AmountPlayerVehicles; v++){
	    set(str,VehicleFile(v));
		if(fexist(str)){
			if(vStats[v][Confiscado] == 1){
				if(pStats[playerid][Prof] != COP_MILITAR && pStats[playerid][Prof] != COP_RODOVIARIO && pStats[playerid][Prof] != COP_SWAT && pStats[playerid][Prof] != DELEGADO && pStats[playerid][Prof] != COP_NARCOTICOS && pStats[playerid][Prof] != EXERCITO){
