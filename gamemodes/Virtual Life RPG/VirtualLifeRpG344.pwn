stock GetValueKey(string[])
{
    new tmp[128];
	tmp[0]=0;
	new sf = strfind(string,"=",true);
	if(sf == -1) return tmp;
	strmid(tmp,string,0,sf);
	return tmp;
}
stock GetKeyValue(string[])
{
    new tmp[128];
	tmp[0]=0;
	new sf = strfind(string,"=",true);
	if(sf == -1) return tmp;
	new sf2 = strfind(string,"\r",true);
	if(sf2==-1){
		strmid(tmp,string,sf+1,strlen(string));
	}
	else{
        strmid(tmp,string,sf+1,sf2);
	}
	return tmp;
}
