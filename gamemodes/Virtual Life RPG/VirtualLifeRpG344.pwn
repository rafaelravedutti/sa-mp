public AvisoTempoRestante(playerid){
	if(pStats[playerid][Preso] == 1){
	    TempoPrisaoFaltando[playerid]--;
		if(TempoPrisaoFaltando[playerid] > 1){
		    new string[128];
		    format(string,sizeof(string),"Restam %d horas para você cumprir sua pena de prisão!",TempoPrisaoFaltando[playerid]);
		    SendClientMessage(playerid, COLOR_COP_SWAT, string);
		    KillTimer(PrisaoTimer[playerid][0]);
		    PrisaoTimer[playerid][0]=SetTimerEx("AvisoTempoRestante",60000,0,"d",playerid);
		    return 1;
		}
		else if(TempoPrisaoFaltando[playerid] == 1){
		    SendClientMessage(playerid, COLOR_COP_SWAT, "Resta apenas 1 hora para você cumprir sua pena de prisão!");
		    KillTimer(PrisaoTimer[playerid][0]);
		    PrisaoTimer[playerid][0]=SetTimerEx("AvisoTempoRestante",60000,0,"d",playerid);
		    return 1;
		}
		else if(TempoPrisaoFaltando[playerid] == 0){
			KillTimer(PrisaoTimer[playerid][0]);
		    SendClientMessage(playerid, COLOR_PALHA, "Solto...");
			SetPlayerFree(playerid);
			return 1;
		}
	}
	return 1;
}
