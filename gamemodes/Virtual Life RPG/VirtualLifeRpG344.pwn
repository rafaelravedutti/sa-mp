stock Salario()
{
	for(new i = 0; i<MAX_SLOTS; i++) {
	    if(IsPlayerConnected(i) && Logado[i] == 1) {
			if(fexist(PlayerFile(PlayerName(i)))){
				SendClientMessage(i, ProfColors[pStats[i][Prof]], StringsSalario[pStats[i][Prof]]);
				pStats[i][BancoTrabalho] += IntSalario[pStats[i][Prof]];
				if(pStats[i][Prof] == COP_SWAT || pStats[i][Prof] == EXERCITO || pStats[i][Prof] == COP_RODOVIARIO || pStats[i][Prof] == COP_MILITAR || pStats[i][Prof] == COP_NARCOTICOS || pStats[i][Prof] == DELEGADO){
					AcrescentarXpoints(i, 7);
				} else if(pStats[i][Prof] == SEGURANÇA || pStats[i][Prof] == MOTORISTA_PARTICULAR){
					AcrescentarXpoints(i, 5);
				}
			}
			else{
				printf("ERRO: %s nao conseguiu receber o salario pois seu arquivo nao foi encontrado pelo sistema.",PlayerName(i));
			}
		}
	}
}

