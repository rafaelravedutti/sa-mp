	SendClientMessageToAll(COLOR_PURPLE, "    ");
	for(new i = 0; i < MAX_SLOTS; i++)
	{
	    if(IsPlayerConnected(i) && PlayerDentro[i])
		{
			for(new j = 0; j < 2; j++)
			{
			    if(Aposta[i][j] == rand1 || Aposta[i][j] == rand2)
				{
			        Acertos[i]++;
			    }
