				}
			}
		}
	}
	return 1;
}
stock ProximosDezPlayers(inicio,&min,&max)
{
	new result, first = -1, last = -1;
	for(new i = inicio; i < SLOTS2; i++){
		if(IsPlayerConnected(i)){
	 		result++;
	 		last = i;
			if(first == -1) first = i;
		}
		if(result >= 10) break;
	}
	if(result == 0) return false;
	min = first;
	max = last;
	return true;
}
