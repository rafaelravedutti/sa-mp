public AumentarHigiene(playerid,pontos){
	if(IsPlayerConnected(playerid) && dini_Exists(PlayerName(playerid))){
	    new string[256];
	    if(dini_Int(PlayerName(playerid),"Higiene") + pontos < 500) {
			dini_IntSet(PlayerName(playerid),"Higiene",dini_Int(PlayerName(playerid),"Higiene") + pontos);
			format(string,sizeof(string),"Seu higiene foi aumentado em %d pontos! Agora você deve estar se sentido mais limpo...",pontos);
			SendClientMessage(playerid, COLOR_GREEN, string);
			format(string,sizeof(string),"Seu higiene: %d",dini_Int(PlayerName(playerid),"Higiene"));
			SendClientMessage(playerid, COLOR_YELLOW, string);
		}
		else{
			dini_IntSet(PlayerName(playerid),"Higiene",500);
			SendClientMessage(playerid, COLOR_GREEN, "Seu higiene está completo, você está até brilhando, não se assuste, foi só força de expressão! ^^");
		}
	}
}
public DiminuirHigiene(playerid,pontos){
	if(dini_Exists(PlayerName(playerid))){
		if(dini_Int(PlayerName(playerid),"Micose") == 0 && Logado[playerid] == 1){
			dini_IntSet(PlayerName(playerid),"Higiene",dini_Int(PlayerName(playerid),"Higiene")-1);
			if(dini_Int(PlayerName(playerid),"Higiene") >= 20 && dini_Int(PlayerName(playerid),"Higiene") <= 30){
				SendClientMessage(playerid, COLOR_RED2, "Sua higiene está baixa, você precisa usar o banheiro.");
			} else if(dini_Int(PlayerName(playerid),"Higiene") >= 10 && dini_Int(PlayerName(playerid),"Higiene") <= 19){
				SendClientMessage(playerid, COLOR_RED2, "Você precisa usar o banheiro imediatamente! Sua higiene está muito baixa, poderá contrair uma doença.");
			} else if(dini_Int(PlayerName(playerid),"Higiene") <= 0){
                dini_IntSet(PlayerName(playerid),"Higiene",0);
                dini_IntSet(PlayerName(playerid),"Micose", 1);
				SetPlayerWeather(playerid,-37);
				SetTimerEx("EstouMicosado",30000,0,"d",playerid);
				SendClientMessage(playerid, COLOR_RED2, "Você não cuidou de seu higiene pessoal e agora pegou uma coceira ( micose ), chame um paramédico para lhe passar os medicamentos.");
				SendClientMessage(playerid, COLOR_RED2, "ou vá a um hospital comprar os medicamentos.");
				if(dini_Int(PlayerName(playerid),"DosesRemed") <= 1){
					dini_IntSet(PlayerName(playerid),"DosesRemed",dini_Int(PlayerName(playerid),"DosesRemed") + 1);
    			}
			}
		}
	}
}
public PerdiHigiene(){
    for(new i = 0; i < SLOTS2; i++){
		DiminuirHigiene(i,1);
	}
}
public EstouMicosado(playerid){
