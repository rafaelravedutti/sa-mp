stock compare(string_text1[],string_text2[])
{
	if(!strlen(string_text1) || !strlen(string_text2)) return 0;
	new b;
	for(new i; i < strlen(string_text1); i+=1){
	    if(strcmp(string_text1[i],string_text2[i],false) == 0) b = 1;
	    else{ b = 0; break; }
	}
	return b;
}
