	SetPlayerCheckpoint(playerid,2242.1575,-1663.0786,15.4766,4.0);
	CheckpointStatus[playerid] = TRANSPORTE_ROUPAS;
}
public OnPlayerStateChange(playerid, newstate, oldstate)
{
	if(newstate == PLAYER_STATE_ONFOOT) TextDrawDestroy(Speedo[playerid]);
	if(newstate == PLAYER_STATE_DRIVER){
	    new vinfo[256];
	    new string[256];
	    format(vinfo,sizeof(vinfo),"%d.veh",GetPlayerVehicleID(playerid));
	    if(dini_Isset(vinfo,"Chave")){
	    	if(!compare(dini_Get(vinfo,"Chave"),PlayerName(playerid)) && !UsingChaveMestra[playerid][GetPlayerVehicleID(playerid)]){
			    if(dini_Int(PlayerName(playerid),"ChaveMestra") > 0){
			    	TogglePlayerControllable(playerid,false);
			    	SendClientMessage(playerid, COLOR_WHITE, "Você tem uma chave mestra, para usa-la digite /usarchavemestra e para não usa-la digite /naousarchavemestra");
					PChaveMestra[playerid] = 1;
				} else {
					if(!strcmp(dini_Get(vinfo,"Dono"), PlayerName(playerid), false)){
	    				SendClientMessage(playerid, COLOR_GREEN, "Veículo ligado.");
				        SendClientMessage(playerid, COLOR_RED, "Você é o dono deste veículo, está usando a chave reserva.");
						format(string,sizeof(string),"A chave Original está com: %s.", dini_Int(vinfo,"Chave"));
						SendClientMessage(playerid, COLOR_RED, string);
					}
				    if(!compare(dini_Get(vinfo,"Chave"),PlayerName(playerid))){
						TogglePlayerControllable(playerid, 0);
						SendClientMessage(playerid, COLOR_RED, "Você não possui as chaves do veículo para liga-lo.");
						SendClientMessage(playerid, COLOR_RED, "DICA: Compre uma chave mestra e tente abrir.");
						SendClientMessage(playerid, COLOR_WHITE, "Para sair do veículo pressione ENTER ou F");
					}
				}
			} else {
			    SendClientMessage(playerid, COLOR_GREEN, "Veículo ligado.");
	 			if(!strcmp(dini_Get(vinfo,"Dono"), PlayerName(playerid), false)){
					if(dini_Int(vinfo,"Dividas") > 0){
					    if(dini_Int(vinfo,"AlertadoMulta") == 0){
				    		SendClientMessage(playerid, COLOR_LIGHT_YELLOW, "[INFO] Seu veículo está com multas atrasadas!");
				    		SendClientMessage(playerid, COLOR_LIGHT_YELLOW, "[INFO] Seu veículo sendo alertado 2x por um oficial será apreendido.");
				    		SendClientMessage(playerid, COLOR_LIGHT_YELLOW, "[INFO] Aconselhamos que pague!");
						} else if((dini_Int(vinfo,"AlertadoMulta") != DiaJogo) && (dini_Int(vinfo,"AlertadoMulta") != 0)){
				    		SendClientMessage(playerid, COLOR_LIGHT_YELLOW, "[INFO] Seu veículo está com multas atrasadas!");
				    		SendClientMessage(playerid, COLOR_LIGHT_YELLOW, "[INFO] Seu veículo já foi alertado, próxima vez poderá ser preso.");
				    		SendClientMessage(playerid, COLOR_LIGHT_YELLOW, "[INFO] Aconselhamos que pague!");
						}
					}
				}
				if(!compare(PlayerName(playerid),dini_Get(vinfo,"dono"))){
				    SendClientMessage(playerid, COLOR_RED, "Cuidado! Esse veículo não é seu, portanto olhe lá hein?!");
				}
			}
