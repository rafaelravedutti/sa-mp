stock CriarPastaUtilitarios(pattern[])
{
	if(fexist(pattern)){
	writelog("Criando pasta de utilitários do gamemode... A pasta não foi criada pois já existia!");
	return 0;
	} else {
	dini_Create(pattern);
	writelog("Criando pasta de utilitários do gamemode... Arquivo criado com sucesso!");
	dini_IntSet(pattern,"Hora",2);
	dini_IntSet(pattern,"Minuto",0);
	dini_IntSet(pattern,"Dia",2);
	dini_IntSet(pattern,"Mes",5);
	dini_IntSet(pattern,"Ano",2008);
 	DiaJogo = 2;
	MesJogo = 5;
	AnoJogo = 2008;
	ValorDia = 4;
	PrecoGas= 1;
	ValorTV = 2300;
	ValorRadio = 400;
	ValorGeladeira = 1800;
	ValorFogao = 1500;
	ValorCelular = 500;
	ValorTelefone = 300;
	ValorFrango = 30;
	ValorPeixe = 40;
	ValorPizza = 25;
	ValorSalada = 15;
	ValorRefrigerante = 12;
	ValorSuco = 18;
	ValorCarne = 35;
	ValorStrogonoff = 45;
	ValorCamarao = 50;
	ValorLinguica = 30;
	HoraJogo = 2;
	MinutoJogo = 0;
	writelog("Dados: Dia da semana, Hora, Minuto, Segundo, Dia, Mes e Ano foram setados na pasta de utilitários do gamemode...");
	return 1;
	}
}
