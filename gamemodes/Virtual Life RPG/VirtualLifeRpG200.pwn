							format(string, sizeof(string), "SERVIDOR: %s passou no radar à %d KM/H e foi multado, o governo/servidor cobriu a multa por ser um veículo que pertence a ele",PlayerName(i), velocidade);
							SendClientMessageAdmin(COLOR_LIGHT_YELLOW, string);
							print(string);
							writelog(string);
							ZeraMulta[i] = 1;
							PlayerPlaySound(i, 1057, 0.0, 0.0, 0.0);
							SetTimerEx("CancelarMulta",5000,0,"d",i);
						}
					    else{
					    	format(strveh,sizeof(strveh),"%d.veh",GetPlayerVehicleID(i));
							format(string, sizeof(string), "Você passou no radar à %d KM/H, e levou uma multa de $%d por estar acima da velocidade permitida!", velocidade, desconto);
							SendClientMessage(i, COLOR_RED, string);
							SendClientMessage(i, COLOR_RED, "Para paga-la vá até a Delegacia!");
							dini_IntSet(strveh, "Dividas", dini_Int(strveh,"Dividas") + desconto);
							ZeraMulta[i] = 1;
							PlayerPlaySound(i, 1057, 0.0, 0.0, 0.0);
							SetTimerEx("CancelarMulta",5000,0,"d",i);
						}
					}
				}
			}
  			if(velocidade <= 120 && GetPlayerState(i) == PLAYER_STATE_DRIVER && !IsPlayerInBike(i) && !IsPlayerInVehicleAir(i)){
			    if(ZeraMulta[i] == 0){
			        PlayerPlaySound(i, 1057, 0.0, 0.0, 0.0);
