public SetCar(modelid,Float:X,Float:Y,Float:Z,Float:ang,Cor1,Cor2,e)
{
	if(!IsPlayerConnected(e)) return false;
	new resid,tmp[256],own[256],strkey[256];
	format(strkey,sizeof(strkey),"VehicleID_%d",dini_Int(PlayerName(e),"LastVeh"));
	resid = CreateVehicle(modelid,X,Y,Z,ang,Cor1,Cor2,-1);
	dini_IntSet(PlayerName(e),strkey,resid);
	dini_IntSet(PlayerName(e),"LastVeh",dini_Int(PlayerName(e),"LastVeh") + 1);
	set(own,PlayerName(e));
	set(Dono[resid],own);
    format(tmp,256,"%d.veh",resid);
    dini_Create(tmp);
    dini_Set(tmp,"dono",own);
	new strplaca[256];
	format(strplaca,sizeof(strplaca),"RVL %d",resid);
	dini_Set(tmp,"Placa",strplaca);
	dini_Set(tmp,"Chave",PlayerName(e));
	dini_IntSet(tmp,"TemAlarme",0);
	dini_IntSet(tmp,"Vendendo",0);
	dini_IntSet(tmp,"PrecoVenda",0);
	switch(modelid){
		case 481,509,510,594:
		{
		   dini_IntSet(tmp,"Dividas",0);
	       dini_IntSet(tmp,"Confiscado",0);
		   dini_IntSet(tmp,"gasolina",-1);
	       GPV[resid]=-1;
		}
		default:
	    {
	        dini_IntSet(tmp,"Dividas",0);
	        dini_IntSet(tmp,"Confiscado",0);
	        dini_IntSet(tmp,"gasolina",100);
	        GPV[resid]=100;
		}
	}
	format(strkey,sizeof(strkey),"Esse é seu veículo de ID %d",dini_Int(PlayerName(e),"LastVeh") - 1);
	SendClientMessage(e, COLOR_WHITE, strkey);
	return true;
}
