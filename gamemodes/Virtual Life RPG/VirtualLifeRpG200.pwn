public ImpostoBanco()
{
	for(new i=0; i<SLOTS2; i++){
	    if(IsPlayerConnected(i)){
		    new string[256];
		    new impostobanco;
		    if(dini_Int(PlayerName(i),"Banco") > 0){
		   		impostobanco = dini_Int(PlayerName(i),"Banco")/44;
		        dini_IntSet(PlayerName(i),"Banco", dini_Int(PlayerName(i),"Banco") - impostobanco);
		   	}
		    else if(dini_Int(PlayerName(i),"Banco") > 20000){
				impostobanco = dini_Int(PlayerName(i),"Banco")/40;
		        dini_IntSet(PlayerName(i),"Banco", dini_Int(PlayerName(i),"Banco") - impostobanco);
			}
			else if(dini_Int(PlayerName(i),"Banco") > 100000){
				impostobanco = dini_Int(PlayerName(i),"Banco")/36;
		        dini_IntSet(PlayerName(i),"Banco", dini_Int(PlayerName(i),"Banco") - impostobanco);
			}
			else if(dini_Int(PlayerName(i),"Banco") > 200000){
				impostobanco = dini_Int(PlayerName(i),"Banco")/32;
		        dini_IntSet(PlayerName(i),"Banco", dini_Int(PlayerName(i),"Banco") - impostobanco);
			}
			format(string, sizeof(string), "[BANCO] A Taxa de juros do banco chegou valor: $%d. E Foi debitada automaticamente na sua conta. Grato, Gerência.", impostobanco);
			SendClientMessage(i, COLOR_DELEGADO, string);
		}
	}
}


