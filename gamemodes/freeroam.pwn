public OnPlayerDisconnect(playerid,reason)
{
InDM1[playerid] = 0;
InDM2[playerid] = 0;
InDM3[playerid] = 0;
InDM4[playerid] = 0;
InDM5[playerid] = 0;
status[playerid][0] = 0;
status[playerid][1] = 0;
status[playerid][2] = 0;
for(new i = 0; i < sizeof(Objects); i++)
{
if(Player[playerid][view][i])
{
Player[playerid][view][i] = false;
DestroyPlayerObject(playerid,Player[playerid][objid][i]);
}
}
return 1;
}
